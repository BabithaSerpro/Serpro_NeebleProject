<AnchorPane fx:id="pAnchor" prefHeight="320.0" prefWidth="350.0" stylesheets="@addPatient.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="addPatient.SuccessController">
   <children>
      <AnchorPane fx:id="cAnchor" layoutY="58.0" prefHeight="262.0" prefWidth="350.0" style="-fx-background-color: white;">
         <children>
            <Button fx:id="ok" layoutX="23.0" layoutY="196.0" mnemonicParsing="false" onAction="#okBtn" prefHeight="37.0" prefWidth="312.0" style="-fx-background-color: #2eacd2;" text="OK" textFill="WHITE" />
            <Label layoutX="140.0" layoutY="64.0" prefHeight="30.0" prefWidth="91.0" text="Success" textFill="#2eacd2">
               <font>
                  <Font name="FontAwesome Regular" size="24.0" />
               </font>
            </Label>
            <Label layoutX="48.0" layoutY="108.0" prefHeight="27.0" prefWidth="286.0" text="Patient Registration is Successful!" textFill="#626568">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label layoutX="36.0" layoutY="136.0" prefHeight="27.0" prefWidth="286.0" text="Please note the Patient ID Number." textFill="#626568">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label layoutX="44.0" layoutY="164.0" prefHeight="27.0" prefWidth="118.0" text="  &quot;Patient ID&quot; :" textFill="#626568">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Label fx:id="pId" layoutX="167.0" layoutY="160.0" prefHeight="30.0" prefWidth="160.0" textFill="#626568">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Circle fill="#2eacd2" layoutX="176.0" layoutY="58.0" radius="51.0" stroke="#2eacd2" strokeType="INSIDE" />
      <ImageView fitHeight="63.0" fitWidth="69.0" layoutX="144.0" layoutY="26.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/done@2x.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
